{
    "elasticsearch": {
      "metadata": {
        "cpuinfo-metadata": {
          "additional_fields": [
            "pod_name"
          ],
          "fields": [
            "value.Model name",
            "value.Architecture",
            "value.CPU(s)"
          ]
        },
        "meminfo-metadata": {
          "additional_fields": [
            "pod_name"
          ],
          "fields": [
            "value.MemTotal"
          ]
        }
      },
      "ripsaw-uperf-results": [
        {
          "filter": {
            "test_type.keyword": "stream"
          },
          "exclude": {
            "norm_ops": 0
          },
          "buckets": [
            "run_id.keyword",
            "uuid.keyword",
            "protocol.keyword",
            "message_size",
            "num_threads",
            "num_pairs.keyword"
          ],
          "aggregations": {
            "norm_byte": [
              "avg"
            ]
          }
        },
        {
          "filter": {
            "test_type.keyword": "rr"
          },
          "exclude": {
            "norm_ops": 0
          },
          "buckets": [
            "run_id.keyword",
            "uuid.keyword",
            "protocol.keyword",
            "message_size",
            "num_threads",
            "num_pairs.keyword"
          ],
          "aggregations": {
            "norm_ops": [
              "avg"
            ],
            "norm_ltcy": [
              {
                "percentiles": {
                  "percents": [
                    99
                  ]
                }
              },
              "avg"
            ]
          }
        }
      ],
      "system-metrics": [
          {
              "filter": {
                  "metricName.keyword": "allNodeCPU"
              },
              "buckets": [
                  "labels.mode.keyword"
              ],
              "aggregations": {
                  "value": [
                      "avg"
                  ]
              }
          },
          {
              "filter": {
                  "metricName.keyword": "nodeCPU"
              },
              "buckets": [
                  "uuid.keyword",
                  "labels.node.keyword",
                  "labels.mode.keyword"
              ],
              "aggregations": {
                  "value": [
                      "avg"
                  ]
              }
          }

      ]
    }
  }
  